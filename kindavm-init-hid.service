[Unit]
Description=KindaVM HID Initialization
After=sys-kernel-config.mount systemd-modules-load.service
Requires=sys-kernel-config.mount systemd-modules-load.service
Before=multi-user.target

[Service]
Type=oneshot
ExecStart=/usr/local/bin/kindavm-init-hid.sh
RemainAfterExit=yes

[Install]
WantedBy=multi-user.target
